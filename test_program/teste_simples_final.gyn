int num1, num2, maior, menor;

int encontra_maior(int a, int b, int c) {
    a = a + 1;
    
    se (a >= b) entao {
        se (a >= c) entao {
            retorne a;
        } senao {
            retorne c;
        }
    } senao {
        se (b >= c) entao {
            retorne b;
        } senao {
            retorne c;
        }
    }
}

